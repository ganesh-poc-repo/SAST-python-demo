def sample(c,a,b):
    if c<=4:
        if c==1:
            print(a+b)
        elif c==2:
            print(a-b)
        elif c==3:
            print(a*b)
        elif c==4:
            print(a//b)
    else:
        print("Invalid Choice")
a,b=map(int,input().split())
c=int(input("Enter Choice:"))
sample(c,a,b)
